{
   "action": {
      "default_title": "Organic + IG List Collector",
      "default_popup": "lovable-popup.html"
   },
   "background": {
      "service_worker": "backgroundscript.js"
   },
   "browser_specific_settings": {
      "gecko": {
         "id": "{ffde70c6-9412-46cf-9643-5fbab7f862e1}"
      }
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "theme.blue.css", "contentscript.css", "jquery-ui.css", "collector.css" ],
      "js": [
         "jquery3.js", "jquery-ui.js", "wNumb.js", "nouislider.js",
         "jquery.tablesorter.js", "jquery.tablesorter.widgets.js", "widget-pager.js",
         "contentscript.js",
         "lovable-config.js", "lovable-logger.js", "lovable-http.js",
         "lovable-supabase.js", "lovable-safety.js", "lovable-sync.js",
         "collector.js",
         "organic-iglc-bridge.js"
      ],
      "matches": [ "https://*.instagram.com/*", "https://*.organicforfollowers.com/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": {
      "extension_pages": "object-src 'self'; script-src 'self'; frame-src 'self' https://www.organicforfollowers.com https://organicpublic.lovable.app https://organicbot.lovable.app;"
   },
   "default_locale": "en",
   "description": "__MSG_appDesc__",
   "host_permissions": [ "https://*.instagram.com/*", "https://*.organicforfollowers.com/*", "https://*.supabase.co/*" ],
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png",
      "48": "icon_48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_appName__",
   "permissions": [ "tabs", "scripting", "storage", "unlimitedStorage", "alarms", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "8.1.2",
   "web_accessible_resources": [ {
      "matches": [ "https://*.instagram.com/*", "https://*.organicforfollowers.com/*" ],
      "resources": [ "*.js", "*.png", "*.css", "*.json", "*.html" ]
   } ]
}
